{
  "parser": "babel-eslint",
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
      "no-plusplus": "off",
      "no-nested-ternary": "off",
      "import/prefer-default-export": "off",
      "no-console": ["error", { "allow": ["error"] }],
      "no-unused-vars": "warn"
  },
  "env": {
    "jest": true
  }
}
